[elrepo]
name            = ELRepo.org Community Enterprise Linux Repository
baseurl         = https://elrepo.org/linux/elrepo/el$releasever/$basearch/
                  https://mirrors.coreix.net/elrepo/elrepo/el$releasever/$basearch/
                  https://jur-linux.org/download/elrepo/elrepo/el$releasever/$basearch/
                  https://repos.lax-noc.com/elrepo/elrepo/el$releasever/$basearch/
mirrorlist      = https://mirrors.elrepo.org/mirrors-elrepo.el$releasever
gpgkey          = https://elrepo.org/RPM-GPG-KEY-elrepo.org
gpgcheck        = 1
protect         = 0
enabled         = 0

