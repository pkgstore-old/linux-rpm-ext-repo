mirrorlist      = https://mirrors.elrepo.org/mirrors-elrepo-kernel.el$releasever
gpgkey          = https://elrepo.org/RPM-GPG-KEY-elrepo.org
gpgcheck        = 1
protect         = 0
enabled         = 0

[elrepo-extras]
name            = ELRepo.org Community Enterprise Linux Extras Repository
